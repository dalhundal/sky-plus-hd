<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/css" href="scpd.css"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
    <specVersion>
        <major>1</major>
        <minor>0</minor>
    </specVersion>
    <actionList>
        <action>
            <name>GetCompatibleUIs</name>
